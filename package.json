{
  "name": "designing-with-search-data",
  "version": "0.0.4",
  "description": "Plugin for Adobe XD - A search engine running inside XD to easily populate your search application design. ",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "npm run compile-for-XD && npm run emptyPlugin && npm run copyFiles",
    "compile-for-XD": "webpack",
    "emptyPlugin": "rm /Users/eklem/Library/Application\\ Support/Adobe/Adobe\\ XD/develop/dist/*",
    "copyFiles": "cp {main.js,manifest.json,XD-webpacked.js} /Users/eklem/Library/Application\\ Support/Adobe/Adobe\\ XD/develop/dist/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eklem/designing-with-search-data.git"
  },
  "keywords": [
    "designing-with-data",
    "search",
    "adobe-xd",
    "search-engine"
  ],
  "dependencies": {
    "encoding-down": "^6.3.0",
    "fergies-inverted-index": "7.0.0",
    "levelup": "^4.4.0",
    "memdown": "^5.1.0",
    "search-index": "1.0.6",
    "webpack": "^5.0.0",
    "webpack-cli": "^4.1.0"
  },
  "author": "Espen Klem",
